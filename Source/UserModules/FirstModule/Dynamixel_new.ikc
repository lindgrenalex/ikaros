    <module
        class = "Constant"
        name = "MovingSpeed"
        outputsize = "1"
        data = "0.4"
    
    />
    
    <connection
        sourcemodule = "MovingSpeed"
        source = "OUTPUT"
        targetmodule = "Dynamixel1"
        target = "MOVING_SPEED"
        
    />
    
    <connection
        sourcemodule = "MovingSpeed"
        source = "OUTPUT"
        targetmodule = "Dynamixel2"
        target = "MOVING_SPEED"
        
    />
    
    <connection
        sourcemodule = "MovingSpeed"
        source = "OUTPUT"
        targetmodule = "Dynamixel1"
        target = "MOVING_SPEED"
    
    />
    
    
    <view name="Dynamixel Control" object_size="50" >
        
        <object
        class="BarGraph"
        title="Actual Positions"
        module = "Servo1"
        source = "data"
        x="0" y="0"
        w="2" h="2"
        max="360"
        />
        
        <object
        class="BarGraph"
        title="Speed"
        module = "MovingSpeed"
        source = "OUTPUT"
        x="2" y="0"
        w="2" h="2"
        max="1"
        />
        
        <object
        class="BarGraph"
        title="Temperature (Â°C)"
        module = "Dynamixel1"
        source = "FEEDBACK_PRESENT_TEMPERATURE"
        x="4" y="0"
        w="2" h="2"
        max="100"
        />
        
        <object
        class="Plot"
        title="Load"
        module = "Dynamixel1"
        source = "FEEDBACK_PRESENT_LOAD"
        x="6" y="0"
        w="2" h="2"
        min="-1"
        max="1"
        />
        <object
        class="BarGraph"
        title="Voltage (V)"
        module = "Dynamixel1"
        source = "FEEDBACK_PRESENT_VOLTAGE"
        x="8" y="0"
        w="2" h="2"
        max="20"
        />
        
        <object
        class="Slider"
        title="Pos S1"
        module = "Servo1"
        parameter = "data"
        select = "0"
        x="0" y="2" h="3"
        max="330"
        min="30"
        />
        
        <object
        class="Slider"
        title="Pos S2"
        module = "Servo2"
        parameter = "data"
        select = "0"
        x="1" y="2" h="3"
        max="200"
        min="30"
        />
        
        <object
        class="Slider"
        title="Pos S3"
        module = "Servo3"
        parameter = "data"
        select = "0"
        x="2" y="2" h="3"
        max="220"
        min="60"
        />
        
        <object
        class="Slider"
        title="Speed"
        module = "MovingSpeed"
        parameter = "data"
        xindex = "0"
        x="3" y="2" h="3"
        max="1"
        />
        
        <object
        class="Table"
        module = "Dynamixel1"
        source = "FEEDBACK_PRESENT_POSITION"
        x="0" y="5"
        w="3" h="2"
        font_size = "30"
        />
        
        <object
        class="Table"
        module = "Dynamixel2"
        source = "FEEDBACK_PRESENT_POSITION"
        x="3" y="5"
        w="3" h="2"
        font_size = "30"
        />
        
        <object
        class="Table"
        module = "Dynamixel3"
        source = "FEEDBACK_PRESENT_POSITION"
        x="6" y="5"
        w="3" h="2"
        font_size = "30"
        />
        
        <object
        class="Switch"
        module="Switch"
        parameter="data"
        x="0"
        y="7"
        w="3"
        />


    </view>
</group>

