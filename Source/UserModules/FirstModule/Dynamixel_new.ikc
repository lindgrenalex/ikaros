<?xml version="1.0"?>

<group title="Dynamixel Example">
    
    
    <module
        class = "Dynamixel"
        name = "Dynamixel1"
        device  = "/dev/cu.usbserial-A4012AK3"
        index_mode = "indirect"
        printf_info ="full"
        use_feedback ="true"
        baud_rate = "1000000"
        angle_unit = "degrees"
        servo_id = "1"
    />
    
    <module
        class = "Dynamixel"
        name = "Dynamixel2"
        device  = "/dev/cu.usbserial-A4012AK3"
        index_mode = "indirect"
        printf_info ="full"
        use_feedback ="true"
        baud_rate = "1000000"
        angle_unit = "degrees"
        servo_id = "2"
    />
    
    <module
        class = "Dynamixel"
        name = "Dynamixel3"
        device  = "/dev/cu.usbserial-A4012AK3"
        index_mode = "indirect"
        printf_info ="full"
        use_feedback ="true"
        baud_rate = "1000000"
        angle_unit = "degrees"
        servo_id = "3"
    />
    
    <module
        class = "Constant"
        name = "TorqueEnable"
        outputsize = "1"
        data = "1"
    />
    
    <connection
        sourcemodule = "TorqueEnable"
        source = "OUTPUT"
        targetmodule = "Dynamixel1"
        target = "TORQUE_ENABLE"
        
    />
    
    
    <connection
        sourcemodule = "TorqueEnable"
        source = "OUTPUT"
        targetmodule = "Dynamixel2"
        target = "TORQUE_ENABLE"
        
    />
    
    
    <connection
        sourcemodule = "TorqueEnable"
        source = "OUTPUT"
        targetmodule = "Dynamixel3"
        target = "TORQUE_ENABLE"
    />
     
    <module
        class = "Constant"
        name = "LED"
        outputsize = "1"
        data = "1"
    
    />
    
    <connection
        sourcemodule = "LED"
        source = "OUTPUT"
        targetmodule = "Dynamixel1"
        target = "LED"
    
    />
    
    <connection
        sourcemodule = "LED"
        source = "OUTPUT"
        targetmodule = "Dynamixel2"
        target = "LED"
        
    />
    
    <connection
        sourcemodule = "LED"
        source = "OUTPUT"
        targetmodule = "Dynamixel3"
        target = "LED"
    
    />
    
    <!-- Tau -->
    
    <!--
