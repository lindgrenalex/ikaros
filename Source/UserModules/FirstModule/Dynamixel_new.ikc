    <module
        class       = "FirstModule"
        name        = "StartPosition"
    />
    
    
    <module
        class = "Constant"
        name = "Switch"
        outputsize = "1"
        data = "0"
    
    />
    
    <connection
        sourcemodule = "Switch"
        source = "OUTPUT"
        targetmodule = "StartPosition"
        target = "INPUT"
    
    />
    
    
    <connection
        sourcemodule = "StartPosition"
        source = "OUTPUT1"
        targetmodule = "Dynamixel1"
        target = "GOAL_POSITION"
        
    />
    
    <connection
        sourcemodule = "StartPosition"
        source = "OUTPUT2"
        targetmodule = "Dynamixel2"
        target = "GOAL_POSITION"
    
    />
    
    <connection
        sourcemodule = "StartPosition"
        source = "OUTPUT3"
        targetmodule = "Dynamixel3"
        target = "GOAL_POSITION"
    
    />
    
